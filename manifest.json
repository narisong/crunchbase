{
  "name": "Crunchbase People",
  "description": "Easily navigate to people's LinkedIn page from Crunchbase.",
  "version": "0.0.2",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.crunchbase.com/organization/*/people"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/logo.png"
    }
  },
  "icons": {
    "16": "/images/logo.png"
  }
}